generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id       Int      @id @default(autoincrement())
  nombre   String
  email    String   @unique
  password String
  tipo     String
  creadoEn DateTime @default(now())
  // Otros campos...
}